<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CWS_Zuora_Product_Available__c</fullName>
    <description>Product Available for Integration</description>
    <externalId>false</externalId>
    <formula>AND(
IsActive,
NOT(ISBLANK(CWS_ProductCode__c)), 
NOT(ISBLANK(TEXT(CWS_ChargeType__c)))
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Indicates if the product is ready to be integrated with Zuora after all required fields are populated</inlineHelpText>
    <label>Zuora Product Available</label>
    <trackHistory>false</trackHistory>
    <type>Checkbox</type>
</CustomField>
