<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CWS_Evergreen_Order__c</fullName>
    <description>If all the Order Line Items are of Type Evergreen, mark the Order as Evergreen</description>
    <externalId>false</externalId>
    <formula>if( CWS_Number_of_Order_Lines__c =  CWS_Number_of_Evergreen_Products__c, true,false )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Evergreen Order</label>
    <trackHistory>false</trackHistory>
    <type>Checkbox</type>
</CustomField>
