<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Monthly_minimum_Commitment_cant_be_null</fullName>
    <active>true</active>
    <errorConditionFormula>ISNULL( CWS_Monthly_Minimum_Commit__c )  || (CWS_Monthly_Minimum_Commit__c &lt; 0 )</errorConditionFormula>
    <errorDisplayField>CWS_Monthly_Minimum_Commit__c</errorDisplayField>
    <errorMessage>Monthly Minimum Commitment cant be null or Negative. It should be 0 or actual value.</errorMessage>
</ValidationRule>
