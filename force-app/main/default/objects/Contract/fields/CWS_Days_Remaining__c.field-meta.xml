<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CWS_Days_Remaining__c</fullName>
    <description>New Field to capture the Days From Expiration of Contract.</description>
    <externalId>false</externalId>
    <formula>IF(SBQQ__ExpirationDate__c!=TODAY()&amp;&amp; NOT(ISNULL(SBQQ__ExpirationDate__c)),(SBQQ__ExpirationDate__c-TODAY()),0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Days Remaining</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
